}
.detailpics {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	flex-wrap: wrap;
	width: calc(648rpx + 12px);
	margin: 0 auto;
	margin-bottom: 9px;
}
.detailpic {
