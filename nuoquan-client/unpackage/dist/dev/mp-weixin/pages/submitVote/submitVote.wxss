
page {
	height: 100%;
}
.submitVoteMain {
	width: 698rpx;
	margin: auto;
}
.title {
	width: calc(698rpx - 36px);
	height: 38px;
	border: 2px solid #FCC041;
	border-radius: 8px;
	padding-left: 12px;
	padding-right: 24px;
}
.title-placeholder {
	height: 14px;
	font-size: 14px;
	font-family: Source Han Sans CN;
	font-weight: 400;
	line-height: 16px;
	color: rgba(199, 199, 199, 1);
	letter-spacing: 17px;
}
.titleTextLeft {
	position: absolute;
	right: 8px;
	top: 15px;
	width: 12px;
	height: 11px;
	font-size: 11px;
	font-weight: 400;
	color: rgba(199, 199, 199, 1);
}
.addContentButton {
	display: inline-block;
	width: 100px;
	margin: 16px 0 0 0;
	padding: 6px 32px 6px 10px;
	height: 26px;
	font-size: 14px;
	line-height: 14px;
	color: #FFFFFF;
	background: -webkit-linear-gradient(left, rgba(83, 224, 219, 1) 0%, rgba(72, 177, 233, 1) 100%);
	background: linear-gradient(90deg, rgba(83, 224, 219, 1) 0%, rgba(72, 177, 233, 1) 100%);
	border-radius: 8px;
}
.addContentButton::before {
	width: 13px;
	content: "";
	background: url(data:image/png;base64,);
	background-size: 13px 13px;
	background-repeat: no-repeat;
	position: absolute;
	right: 13px;
	height: 13px;
}
.content {
	min-height: 51px;
	max-height: 300px;
	margin-top: 13px;
	width: calc(100% - 12px);
	overflow: scroll;
	padding: 36px 4px 24px;

	border: 2px solid rgba(252, 192, 65, 1);
	border-radius: 8px;
	font-size: 14px;
}
.picturearea {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	-webkit-box-flex: 0;
	-webkit-flex: 0 0 auto;
	        flex: 0 0 auto;
	margin-top: 10px;
}
.picturearea image {
	width: 190rpx;
	height: 190rpx;
	margin: 6px 0;
}
.addPic {
	width: 178rpx;
	height: 178rpx;
	line-height: 160rpx;
	margin: 6px 0;
	text-align: center;
	vertical-align: middle;
	color: #888888;
	font-size: 50px;
	font-weight: 200;
	background: #ECECEC;
}
.optionItem {
	margin-top:12px;
	width: 634rpx;
	height: 14px;
	font-size: 14px;
	line-height: 14px;
	padding: 24rpx 31rpx;
	border: 1rpx solid rgba(188, 188, 188, 1);
	border-radius: 20px;
	min-height: auto;
}
.addOptionButton {
	margin-top: 13px;
	width: 698rpx;
	height: 14px;
	line-height: 14px;
	font-size: 14px;
	line-height: 14px;
	color: rgba(255, 255, 255, 1);
	background: rgba(72, 177, 233, 1);
	border-radius: 20px;
	padding: 24rpx 0;
	text-align: center;
	box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.16);
}
.submitVoteButton {
	margin-top: 13px;
	width: 698rpx;
	height: 14px;
	line-height: 14px;
	font-size: 14px;
	line-height: 14px;
	color: rgba(255, 255, 255, 1);
	background: rgba(252, 192, 65, 1);
	border-radius: 20px;
	padding: 24rpx 0;
	text-align: center;
	box-shadow: 0px 0px 6px rgba(0, 0, 0, 0.16);
}

