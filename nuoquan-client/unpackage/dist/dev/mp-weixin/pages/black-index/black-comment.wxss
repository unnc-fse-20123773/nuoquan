
.comment{
	width:634rpx;
	margin:auto;
	background: #A6A6A6;
}
.star {
	display: inline-block;
	width: 14px;
	height: 14px;
	color: #ffffff;
	margin-left: 13px;
	margin-top: 10px;
}
.user {
	margin: 16px 25px 0 25px;
	font-size: 10px;
	font: MicrosoftYaHei;
	font-weight: bold;
	line-height: 19px;
	margin: 16px 13px 0 15px;
	display: inline-block;
	color: #D5D5D5;
}
.reply {
	margin: 16px 25px 0 25px;
	font-size: 10px;
	font: MicrosoftYaHei;
	font-weight: bold;
	line-height: 19px;
	margin: 16px 13px 0 15px;
	display: inline-block;
	color: #E7E7E7;
}
.content{
	width: 536rpx;
	margin:12px auto;
	font-size: 13px;
}
.menubar {
	position: relative;
	vertical-align: middle;
	width: 100%;
	height: 13px;
	margin: auto;
	vertical-align: top;
}
.time {
	display: inline-block;
	left: 120px;
	font-size: 10px;
	margin-left: 25px;
	color: #888888;
	vertical-align: top;
	max-width: 85px;
	text-overflow: ellipsis;
}
.icons {
	position: absolute;
	right: 0;
	top: 0;
	height: 13px;
	width: 206rpx;
	text-align: right;
	display: inline-block;
	content-flex: right;
	overflow: hidden;
	border-bottom-right-radius: 8px;
}
.icons image {
	width: 11px;
	height: 11px;
	padding-right: 10px;
	color: #ffffff;
	vertical-align: top;
}
image {
	width: 30%;
	height: 200rpx;
	margin: auto;
}
.dividingLine{
	width:562rpx;
	margin:9px auto 0;
	border-top:1px solid #838383;
	padding-bottom: 3px;
}

