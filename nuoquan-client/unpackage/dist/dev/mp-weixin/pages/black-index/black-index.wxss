.displayArea {
	width: 87.2%;
	margin-top: 5px;
	margin: auto;
}
.search {
	font-size: 15px;
	display: inline-block;
	width: 190px;
	height: 28px;
	vertical-align: middle;
	border-radius: 8px;
	background: white;
	letter-spacing: 1px;
	color: #b2b2b2;
