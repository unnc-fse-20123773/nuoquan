}
.timing {
	height: 86px;
	width: 100%;
}
.timing view {
	display: inline-block;
	color: #b2b2b2;
	margin-top: 27px;
	margin-bottom: 25px;
}
.timingHour,
.timingMin {
	font-size: 25px;
	height: 35px;
	width: 37px;
border-radius: 5PX;
 	background: -webkit-linear-gradient(#4D4D4D 49%, #5F5F5F 49%);
 	background: linear-gradient(#4D4D4D 49%, #5F5F5F 49%);
