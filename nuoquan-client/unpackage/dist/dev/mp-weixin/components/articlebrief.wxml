<view class="articlecard" id="'+articleCard.id+'" data-event-opts="{{[['tap',[['jumpToDetail']]]]}}" bindtap="__e"><view class="title">{{articleCard.articleTitle}}</view><view class="briefarticleCard">{{articleCard.articleContent}}</view><view class="picturearea"><block wx:if="{{articleCard[3]}}"><image src="../static/0001/pic1.jpg"></image></block><block wx:if="{{articleCard[4]}}"><image src="../static/0001/pic2.jpg"></image></block><block wx:if="{{articleCard[5]}}"><image src="../static/0001/pic3.jpg"></image></block></view><view class="tags"><block wx:for="{{articleCard.tags}}" wx:for-item="i" wx:for-index="index" wx:key="index"><view class="tag">{{i}}</view></block></view><view class="menubar"><image class="touxiang" src="../static/touxiang.jpg"></image><view class="name">{{articleCard.nickName}}</view><view class="time">{{articleCard[7]}}</view><view class="icons"><image class="comment" src="../static/icon/comment.svg">{{articleCard.commentNum}}</image><image class="like" src="../static/icon/like.svg">{{articleCard.likeNum}}</image></view></view></view>